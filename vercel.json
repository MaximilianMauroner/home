{
  "headers": [
    {
      "source": "/(.*)?service-worker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=604800, s-maxage=604800, immutable"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/projects",
      "headers": { "cache-control": "s-maxage=60" }
    }
  ]
}
